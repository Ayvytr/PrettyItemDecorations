<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ayvytr.prettyitemdecorations"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StickyHeaderActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".BasicStickyHeaderActivity"
            android:parentActivityName=".StickyHeaderActivity">
        </activity>
        <activity
            android:name=".TestStickyActivity1"
            android:parentActivityName=".StickyHeaderActivity"/>
        <activity
            android:name=".TestStickyActivity2"
            android:parentActivityName=".StickyHeaderActivity"/>
    </application>

</manifest>